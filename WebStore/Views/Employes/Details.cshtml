@{
    Layout = "_Layout";
}
@model WebStore.Models.Employee
<p>Employee Card</p>
<table class="table table-hover">
    <thead>
        <tr>
            <th>First Name</th>
            <th>LastName</th>
            <th>Age</th>
            <th>Gender</th>
            <th>Post</th>
        </tr>
    </thead>
    <tbody>
        <tr>
            <td>@Model.FirstName</td>
            <td>@Model.LastName</td>
            <td>@Model.Age</td>
            <td>@Model.Gender</td>
            <td>@Model.Post</td>
        </tr>
    </tbody>
</table>

<div>
    <p><a href="/Employes/Index">Back to Employes List</a></p>
</div>

